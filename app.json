{
  "expo": {
    "name": "Care Health Plus",
    "slug": "carehealthplus",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/Login.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.spcl.carehealthplus"
    },
    "android": {
      "package": "com.spcl.carehealthplus"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#e36881"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
